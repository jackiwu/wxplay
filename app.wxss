/**app.wxss**/
@import "colorui/main.wxss";
@import 'colorui/icon.wxss';
@import 'colorui/animation.wxss';
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
} 
page{
  overflow-x:hidden;
  width: 100%; 
  background: #fff;
}
::-webkit-scrollbar{
   width: 0;
    height: 0;
    color: transparent;
}